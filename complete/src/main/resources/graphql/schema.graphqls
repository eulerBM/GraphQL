type Query {
    bookById(id: ID): Book
    productById(id: Int): Product
}

type Product {
    id: ID
    name: String
    description: String
    price: Float

}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}